{
    "manifest_version": 3,

    "name": "My First Extension",
    "description": "Learn and build",
    "version": "1.0",
    "permissions": ["storage", "activeTab","scripting","tabs"],
    "host_permissions": ["<all_urls>"],
    
    "action": {
        "default_title": "Click Me",
        "default_popup": "popup.html"
    }

}






